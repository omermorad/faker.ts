{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "esModuleInterop": true,
    "target": "es5",
    "allowJs": true,
    "incremental": false,
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "test"
  ]
}
